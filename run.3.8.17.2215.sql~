SELECT r.MQ_ROWNUM,e.COADD_OBJECT_ID,e.mjd,e.RA,e.DEC,e.mag_psf,e.MAG_PSF_ERROR as MAGERR_PSF,e.mag_auto,e.MAG_AUTO_ERROR as MAGERR_AUTO,e.band,e.flags FROM rumbaugh.milliquas_y3a1_match_pass2 r,rumbaugh.ERIC_LC_Y3A1 e where e.coadd_object_id=r.coadd_objects_id; >milliquas_lightcurve_entries_y3a1_SN.tab
